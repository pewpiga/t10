<Window x:Class="t10.SelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:t10"
        mc:Ignorable="d"
        Title="SelectorWindow" Height="205" Width="535">
    <Grid Margin="10,10,10,10">
        <ComboBox x:Name="selectedRole" Margin="0,69,0,0" Width="215" Height="22" SelectedIndex="1" HorizontalAlignment="Center" VerticalAlignment="Top">
            <ComboBoxItem Content="Консультант"/>
            <ComboBoxItem Content="Менеджер"/>
        </ComboBox>
        <Button Content="Подтвердить" HorizontalAlignment="Center" Margin="0,119,0,0" VerticalAlignment="Top" Width="215" Click="Button_Click"/>

    </Grid>
</Window>
